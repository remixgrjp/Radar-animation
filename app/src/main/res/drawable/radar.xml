<?xml version="1.0" encoding="utf-8"?>
<layer-list
	xmlns:android="http://schemas.android.com/apk/res/android"
>
	<item
		android:top="-2dp"
		android:right="-2dp"
		android:left="-2dp"
	>
		<shape>
			<stroke
				android:width="2dp"
				android:color="#33FF33"
			/>
			<gradient
				android:type="linear"
				android:startColor="#00000000"
				android:endColor="#2F00FF00"
				android:angle="270"
			/>
		</shape>
	</item>
</layer-list>
